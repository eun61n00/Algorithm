   "source": [
    "t = int(input())\n",
    "for _ in range(t):\n",
    "    h, w, n = map(int, input().split()) #층수, #방수, #몇번째손님\n",
    "    room = n // h + 1\n",
    "    room = room.zfill(2)\n",
    "    floor = n % h\n",
    "    if floor == 0:\n",
    "        floor = 1\n",
    "    print(floor, room, sep='')"
   ]
