    "n = int(input())\n",
    "tips = []\n",
    "answer = 0\n",
    "\n",
    "for _ in range(n):\n",
    "    tips.append(int(input()))\n",
    "    \n",
    "tips.sort(reverse=True)\n",
    "\n",
    "for i in range(n):\n",
    "    final_tip = tips[i] - i\n",
    "    if final_tip < 0:\n",
    "        continue\n",
    "    else:\n",
    "        answer += tips[i] - i\n",
    "    \n",
    "print(answer)"
   ]
